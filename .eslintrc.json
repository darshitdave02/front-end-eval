{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier"
    ],
    "overrides": [
       
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react", "react-hooks", "@typescript-eslint", "prettier"],
    "rules": {
            "react/react-in-jsx-scope": "off",
            "camelcase": "warn",
            "spaced-comment": "warn",
            "quotes": ["warn", "single"],
            "no-duplicate-imports": "warn",
            "no-undef": "warn",
            "no-unused-vars": "warn"
    }
}
